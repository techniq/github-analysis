    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "d3-time": "^3.1.0",
    "date-fns": "^4.1.0",
    "layerchart": "2.0.0-next.14",
    "posthog-js": "^1.248.1",
