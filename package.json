    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "5.33.10",
    "svelte-check": "^4.2.1",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^4.1.8",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-devtools-json": "^0.2.0"
